{
  "name": "baseui-wc-base-component",
  "version": "0.1.0",
  "description": "re-usable base component for custom elements",
  "main": "dist/baseui-wc-base-component.js",
  "module": "src/index.js",
  "scripts": {
    "start": "npm i && npm run server",
    "clean": "test -d 'dist' && rm -r dist/* || echo 'Build folder is clean!'",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "lint": "./node_modules/.bin/eslint  ./src/**/*.js --quiet",
    "webpackServer": "./node_modules/.bin/webpack-dev-server --config webpack.config.js",
    "wpBuild": "npm run clean && ./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js",
    "rollupBuild": "npm run clean && ./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/rollup -c",
    "server": "npm run webpackServer",
    "bundleServ": "./node_modules/.bin/live-server --port=1919 --host=0.0.0.0 --open='./demo/'",
    "release": "npm run lint && npm run rollupBuild && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsadhanala/baseui-wc-base-component.git"
  },
  "keywords": [
    "web component",
    "custom element",
    "redux",
    "base custom element",
    "uses lit-html",
    "uses hyper-html",
    "ES6 template literals",
    "ES6 mixins"
  ],
  "author": "dsadhanala@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsadhanala/wc-base-component/issues"
  },
  "homepage": "https://github.com/dsadhanala/wc-base-component#readme",
  "dependencies": {
    "baseui-wc-utils": "0.1.1",
    "hyperhtml": "^2.4.3",
    "lit-html": "^0.7.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "document-register-element": "^1.7.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jsdoc": "^3.4.2",
    "jsdom": "^11.5.1",
    "karma": "^1.7.1",
    "karma-jsdom-launcher": "^6.1.2",
    "live-server": "^1.2.0",
    "minami": "^1.1.1",
    "open-browser-webpack-plugin": "^0.0.5",
    "prettier-eslint": "^8.7.1",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.3.5",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-dashboard": "0.4.0",
    "webpack-dev-server": "^2.10.1"
  }
}
