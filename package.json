{
  "name": "baseui-wc-base-component",
  "version": "0.4.2",
  "description": "re-usable base customElement to avoid repeated bootstrap code, flexible to choose own rendering or use hyperHTML/LitHTML.",
  "main": "dist/",
  "module": "src/index.js",
  "scripts": {
    "start": "npm i && npm run server",
    "clean": "test -d 'dist' && rm -r dist/* || echo 'Build folder is clean!'",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "lint": "./node_modules/.bin/eslint --fix  ./src/**/*.js --quiet",
    "build": "npm run clean && npm run build-dev && npm run build-min",
    "build-dev": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.prod.js",
    "build-min": "./node_modules/.bin/cross-env NODE_ENV=production ./node_modules/.bin/webpack --env.min --config webpack.prod.js",
    "server": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js",
    "bundleServ": "./node_modules/.bin/live-server --port=1919 --host=0.0.0.0 --open='./demo/'",
    "release": "npm run lint && npm run build && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dsadhanala/baseui-wc-base-component.git"
  },
  "keywords": [
    "web component",
    "custom element",
    "redux",
    "base custom element",
    "uses lit-html",
    "uses hyper-html",
    "ES6 template literals",
    "ES6 mixins"
  ],
  "author": "dsadhanala@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dsadhanala/wc-base-component/issues"
  },
  "homepage": "https://github.com/dsadhanala/wc-base-component#readme",
  "dependencies": {
    "hyperhtml": "^2.8.0",
    "lit-html": "^0.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jsdoc": "^3.4.2",
    "live-server": "^1.2.0",
    "minami": "^1.1.1",
    "prettier-eslint": "^8.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  }
}
